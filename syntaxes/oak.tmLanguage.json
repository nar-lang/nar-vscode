{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "oak",
	"patterns": [
		{
			"include": "#keywords"
		},
		{
			"include": "#strings"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [
				{
					"name": "keyword.oak",
					"match": "\\b(module|import|as|exposing|infix|alias|type|def|hidden|native)\\b"
				},
				{
					"name": "keyword.control.oak",
					"match": "\\b(if|then|else|let|in|select|case|end)\\b"
				}
			]
		},
		"strings": {
			"name": "string.quoted.double.oak",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.oak",
					"match": "\\\\."
				}
			]
		}
	},
	"scopeName": "source.oak"
}
